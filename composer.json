{
  "name": "mike-shu/lara8docs",
  "description": "Russian documentation for Laravel 8.x",
  "type": "project",
  "keywords": [
    "framework",
    "laravel 8",
    "documentation",
    "russian"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mike Shu",
      "email": "heliax44@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "masterminds/html5": "^2.0",
    "nikic/fast-route": "^1.3",
    "smarty/smarty": "~3.1",
    "ext-dom": "*",
    "ext-json": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  }
}
